ition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div mbr-class=\"{'container':!fullWidth, 'container-fluid': fullWidth}\">\n        <!-- Content -->\n        <div class=\"mbr-row\" mbr-class=\"{'align-left': alignTitle==1, 'align-center': alignTitle==2, 'align-right': alignTitle==3}\">\n            <!-- Titles -->\n            <div class=\"title-block mbr-col-sm-12 mbr-col-md-12 mbr-white\" mbr-class=\"{'align-left': alignTitle==1, 'align-center': alignTitle==2, 'align-right': alignTitle==3}\">\n                <h2 class=\"mbr-white mbr-fonts-style mbr-light\" mbr-theme-style=\"display-7\" mbr-class=\"{'mbr-pb-2': showTitle}\" mbr-if=\"showSubtitle\">Request your best Health Support Provider today</h2>\n                <h1 class=\"mbr-white mbr-fonts-style mbr-pb-4 mbr-bold\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">Welcome to Mayer Health Services</h1>\n                <div mbr-buttons class=\"mbr-section-btn mbr-pt-1 btn-custom btn-1\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButtons\"><a class=\"btn-primary btn\" href=\"index.html#reviews1-5\" data-app-placeholder=\"Type Text\">TESTIMONIALS</a></div>\n                <div mbr-buttons class=\"mbr-section-btn mbr-pt-1 btn-custom\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButtons\"><a class=\"btn-danger btn\" href=\"index.html#footer2-6\" data-app-placeholder=\"Type Text\">CONTACT US</a></div>\n            </div>\n        </div>\n    </div>\n</section>",
