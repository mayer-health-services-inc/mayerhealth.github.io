        {
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".title-wrap, .title": {
              "width": "100%"
            },
            "*": {
              "word-break": "break-word"
            },
            ".social-wrap a": {
              "text-decoration": "none"
            },
            "& when (@showIcons)": {
              ".social": {
                ".iconfont-wrapper": {
                  "display": "inline-block",
                  "width": "~\"@{sizeIcon}rem\"",
                  "height": "~\"@{sizeIcon}rem\"",
                  "margin-right": "1rem",
                  "span.amp-iconfont": {
                    "color": "@colorIcon !important",
                    "font-size": "~\"@{sizeIcon}rem\""
                  }
                }
              }
            },
            "h3": {
              "font-weight": "900",
              "letter-spacing": "2px"
            },
            ".column3": {
              ".column3-content": {
                "form": {
                  "input, textarea": {
                    "width": "100%",
                    "margin-bottom": "15px"
                  }
                }
              },
              ".mbr-section-btn": {
                "width": "100%",
                "text-align": "left",
                ".btn": {
                  "padding": "17px 30px",
                  "border-radius": "50px",
                  "transition": "box-shadow 0.3s ease",
                  "margin": "0",
                  "&:hover": {
                    "box-shadow": "0 15px 20px 0 rgba(117, 103, 248, 0.28)"
                  }
                }
              }
            },
            ".input-label": {
              "width": "100%"
            },
            ".area": {
              "width": "100%"
            },
            ".fieldset": {
              "-webkit-justify-content": "unset",
              "justify-content": "unset",
              ".mbr-section-btn": {
                "margin": "0",
                "a": {
                  "margin": "0"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".column1, .column2": {
                "margin-bottom": "15px"
              },
              ".column3 .mbr-section-btn": {
                "text-align": "center"
              }
            }
          },
          "_name": "footer2",
          "_customHTML": "<section class=\"footer2\" group=\"Footers\" plugins=\"amp-main, amp-mustache, amp-form\">\n\t<!--  -->\n\t<mbr-parameters>\n\t\t<header>Size:</header>\n\t\t<input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<!--  -->\n\t\t<header>Column:</header>\n\t\t<input type=\"range\" title=\"Column:\" name=\"countColumn\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n\t\t<input type=\"checkbox\" title=\"Title:\" name=\"showTitleColumn\" checked>\n\t\t<select title=\"Title:\" name=\"alignTitle\" condition=\"showTitleColumn\">\n\t\t\t<option value=\"1\" selected>Left</option>\n\t\t\t<option value=\"2\">Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<!--  -->\n\t\t<header>1 column:</header>\n\t\t<input type=\"checkbox\" title=\"Show Icon:\" name=\"showIcons\">\n\t\t<input type=\"range\" title=\"Icons:\" name=\"countSocialIcon\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"showIcons\">\n\t\t<input type=\"range\" title=\"Size Icon:\" name=\"sizeIcon\" min=\"1\" max=\"3\" step=\"0.1\" value=\"1.5\" condition=\"showIcons\">\n\t\t<input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#ffffff\" condition=\"showIcons\">\n\t\t<!--  -->\n\t\t<header>2 column:</header>\n\t\t<input type=\"range\" title=\"String:\" name=\"countStringCol1\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n\t\t<select title=\"Align:\" name=\"alignCol1\">\n\t\t\t<option value=\"1\" selected>Left</option>\n\t\t\t<option value=\"2\">Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<!--  -->\n\t\t<header>3 column:</header>\n\t\t<input type=\"range\" title=\"String:\" name=\"countStringCol2\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n\t\t<select title=\"Align:\" name=\"alignCol2\">\n\t\t\t<option value=\"1\" selected>Left</option>\n\t\t\t<option value=\"2\">Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<!--  -->\n\t\t<header>Background:</header>\n\t\t<fieldset type=\"background\" name=\"bg\">\n\t\t\t<input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/mbr-1-1920x1280.jpg\" selected>\n\t\t\t<input type=\"color\" title=\"Background Color:\" value=\"#151135\">\n\t\t</fieldset>\n\t\t<input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n\t\t<input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#25ace2\" condition=\"bg.type!='color' && overlay\">\n\t\t<input type=\"range\" inline title=\"Overlay Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"bg.type!='color' && overlay\">\n\t</mbr-parameters>\n\t<!--  -->\n\t<!-- Overlay -->\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n\t<!-- Container -->\n\t<div class=\"container\">\n\t\t<div class=\"mbr-row mbr-pt-4 mbr-white\">\n\t\t\t<!-- 1-column -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-4\">\n\t\t\t\t<div class=\"column1\">\n\t\t\t\t\t<h3 class=\"mbr-fonts-style mbr-bold mbr-pb-4\" mbr-if=\"showTitleColumn\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">Mayer Health Services</h3>\n\t\t\t\t\t<div class=\"column1-content\">\n\t\t\t\t\t\t<p class=\"mbr-fonts-style\" mbr-theme-style=\"display-4\">We provide the very best of Personal Support Services. Punctuality, kindness, and unmatched empathy is what we practice here at Mayer. Rest assured that your loved ones or yourself will receive the very best of care.</p>\n\t\t\t\t\t\t<div class=\"social-wrap mbr-py-4\" mbr-list mbr-list-grow=\"countSocialIcon\" mbr-if=\"showIcons\">\n\t\t\t\t\t\t\t<a href=\"https://www.facebook.com/Mobirise\" target=\"_blank\" class=\"social\">\n\t\t\t\t\t\t\t\t<span class=\"iconfont-wrapper\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"fa-facebook fa amp-iconfont\"></span>\n\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t\t<a href=\"https://www.instagram.com/mobirise/\" target=\"_blank\" class=\"social\">\n\t\t\t\t\t\t\t\t<span class=\"iconfont-wrapper\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"fa-instagram fa amp-iconfont\"></span>\n\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t\t<a href=\"https://twitter.com/mobirise\" target=\"_blank\" class=\"social\">\n\t\t\t\t\t\t\t\t<span class=\"iconfont-wrapper\">\n\t\t\t\t\t\t\t\t\t<span mbr-icon class=\"fa-twitter fa amp-iconfont\"></span>\n\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<!-- 2-column -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-4\" mbr-if=\"countColumn > 2\">\n\t\t\t\t<div class=\"column2\">\n\t\t\t\t\t<h3 class=\"mbr-fonts-style mbr-bold mbr-pb-4\" mbr-if=\"showTitleColumn\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">LOCATION</h3>\n\t\t\t\t\t<div class=\"column2-content\">\n\t\t\t\t\t\t<p class=\"mbr-pb-2 mbr-fonts-style\" mbr-theme-style=\"display-4\">68 D'amato Crescent, Vaughan ON, L6A 4P2&nbsp;<br></p>\n\t\t\t\t\t\t<p class=\"mbr-pb-2 mbr-fonts-style\" mbr-theme-style=\"display-4\">Email: info@mayerhealth.ca</p>\n\t\t\t\t\t\t<p class=\"mbr-pb-2 mbr-fonts-style\" mbr-theme-style=\"display-4\">24 Hours 7 Days a week</p>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<!-- 3-column -->\n\t\t\t<div class=\"mbr-col-sm-12 mbr-col-md-4\" mbr-if=\"countColumn > 3\">\n\t\t\t\t<div class=\"column3\">\n\t\t\t\t\t<h3 class=\"mbr-fonts-style mbr-bold mbr-pb-4\" mbr-if=\"showTitleColumn\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">DROP US A LINE</h3>\n\t\t\t\t\t<div mbr-f